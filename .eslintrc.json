{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-var": "error",
        "eqeqeq": "error",
        "semi": [
            "error",
            "always"
        ],
        "curly": [
            "warn",
            "all"
        ],
        "prefer-const": [
            "warn",
            {}
        ]
    }
}